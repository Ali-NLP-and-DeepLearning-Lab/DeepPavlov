{
  "chainer": {
    "in": ["manifest"],
    "pipe": [
      {
        "class_name": "nemo_asr",
        "model_path": "{NEMO_PATH}/quartznet15x5/quartznet15x5.yaml",
        "checkpoints_dir": "{NEMO_PATH}/quartznet15x5",
        "in": ["manifest"],
        "out": ["audio_path", "transcription"]
      }
    ],
    "out": ["audio_path", "transcription"]
  },
  "metadata": {
    "variables": {
      "ROOT_PATH": "~/.deeppavlov",
      "NEMO_PATH": "{ROOT_PATH}/models/nemo"
    },
    "requirements": [
      "{DEEPPAVLOV_PATH}/requirements/nemo-asr.txt"
    ],
    "download": [
      {
        "url": "http://files.deeppavlov.ai/deeppavlov_data/nemo/quartznet15x5.tar.gz",
        "subdir": "{NEMO_PATH}"
      }
    ]
  }
}
